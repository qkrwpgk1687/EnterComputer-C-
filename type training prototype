# 금요일에 할꺼 :점수판 레이블 구현 ㅇㅋ?
#label 설정
label = Label(root,width=30)
label.pack(padx=900,pady=0)
time = 120
playerscore = Label(root,width=20)
playerscore.pack()
comscore = Label(root,width = 20)
comscore.pack()
def timer(time):
    if time == 0:
        result()
        return 0
    else:
        label.configure(text="%d 초 남음"%time)
        if time % 3 == 0:
            while(Com()==0):
                continue

    root.after(1000,timer,time-1)
    playerscore.configure(text = "%d"%player)
    comscore.configure(text = "%d"%com)
point = Label(root,width=20)
point.pack()
point.configure(text="너              컴퓨터")
result1 = Label(root,width=20)
result1.pack()
def result():
    if player > com:
        label.configure(text = "승리!",fg = "blue")
    if player < com:
        label.configure(text = "패배!",fg = "red")
    if player == com:
        label.configure(text = "무승부!")
timer(time)


root.mainloop()

