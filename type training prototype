# entry 설정
entry = Entry(root, width=30)
entry.bind("<Return>", solution)
entry.place(x=1300,y=400)
num = 0
def Com():
    num = random.randint(1,225)
    if status[num] == 1:
        return 0
    else:
        status[num] = 1
